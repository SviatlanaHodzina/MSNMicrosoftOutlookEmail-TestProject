<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CrossBrowserSmoke" verbose="1">
    <test name="ChromeLogInPageOpeningTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="org.microsoft.MSNOutlook.tests.LogInPageOpeningTest"/>
        </classes>
    </test>
    <test name="FirefoxLogInPageOpeningTest">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="org.microsoft.MSNOutlook.tests.LogInPageOpeningTest"/>
        </classes>
    </test>
    <test name="MsedgeLogInPageOpeningTest">
        <parameter name="browser" value="msedge"/>
        <classes>
            <class name="org.microsoft.MSNOutlook.tests.LogInPageOpeningTest"/>
        </classes>
    </test>
</suite>